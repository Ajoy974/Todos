            <path
              d="M11 4H4a2 2 0 0 0-2 2v14a2 2 0 0 0 2 2h14a2 2 0 0 0 2-2v-7"
            ></path>
            <path
              d="M18.5 2.5a2.121 2.121 0 0 1 3 3L12 15l-4 1 1-4 9.5-9.5z"
            ></path>
          </svg>
        </li>
      </ul>
      <!-- new list section end -->
    </section>
    <!-- SECTION TWO FOR BODY -->
    <section
      class="sec-2 bg-cover bg-slate-400 bg-center h-screen relative"
      style="
        width: calc(100% - 300px);
        background: url('https://cdn.pixabay.com/photo/2021/07/08/14/06/sea-6397001_1280.jpg');
        border-top-left-radius: 10px;
      "
      id="gallery"
    >
      <div class="tab-content bg-opacity-30 active" id="tab1">
        <!-- Header -->
        <!-- <div
          class="ls justify-center w-20 items-center cursor-pointer"
          onclick="navbar()"
        >
          <i class="bi-three-dots-vertical"></i>
        </div> -->
        <div
          class="top-0 left-0 h-24 flex-col flex items-center justify-between pr-7 backdrop-blur-md bg-opacity-35 text-black m-0"
